{
  "name": "kirby-retour",
  "version": "1.0.0-beta.1",
  "description": "Redirect management for Kirby 3",
  "main": "index.js",
  "author": "Nico Hoffmann",
  "license": "MIT",
  "scripts": {
    "dev": "watchify -vd -p browserify-hmr -e src/main.js -o index.js",
    "build": "browserify -p [ vueify/plugins/extract-css -o index.css ] -p bundle-collapser/plugin -e src/main.js > index.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.3",
    "browserify-hmr": "^0.3.6",
    "bundle-collapser": "^1.3.0",
    "chart.js": "^2.7.3",
    "dayjs": "^1.8.0",
    "node-sass": "^4.9.3",
    "vue": "^2.5.17",
    "vue-chartjs": "^3.4.0",
    "vueify": "^9.4.1",
    "watchify": "^3.11.0"
  }
}
